{
  "version": 2,
  "name": "gymmaster-backend",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**", "src/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },

  "regions": ["iad1"],
  "installCommand": "npm install && npx prisma generate"
}